            function myMap() {
                myCenter = new google.maps.LatLng(24.789071, 120.999645);
                var mapOptions = {
                    center: myCenter,
                    zoom: 12, scrollwheel: true, draggable: true,
                    mapTypeId: google.maps.MapTypeId.ROADMAP
